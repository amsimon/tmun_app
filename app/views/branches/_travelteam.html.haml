  .module
    .title
      %span
        BRANCH SCHEDULES
    .admin-container.add
      = link_to 'javascript:;' do
        .clicker.add +
      %ul.admin-list
        %li
          = link_to url_for([:new, @branch, :schedule]), class: "admin-link" do
            .admin-line
              .icon
                = image_tag 'admin-icons/create.png'
              .words
                Add branch schedule
    .content#important
      = render partial: 'schedules/branch_schedule', collection: @branch.schedules, as: 'schedule', locals: {parent: @branch, conf: "false", parent_name: @branch.class.name.demodulize.downcase}

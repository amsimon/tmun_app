.sub-module.module
  %table.branch-schedule
    %tr
      %td
        = f.hidden_field :_destroy
        = link_to '#', class: "remove_fields admin-link" do
          .event-x
            &times;
      %td
        = f.label :date
        %br/
        = f.date_field :date
      %td
        = f.label :time_start, "Start Time"
        %br/
        = f.time_field :time_start
        %br/
        = f.label :time_end, "End Time"
        %br/
        = f.time_field :time_end
      %td
        = f.label :activity
        %br/
        = f.text_field :activity
      %td
        = f.label :place
        %br/
        = f.text_field :place


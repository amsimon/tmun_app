- provide(:title, 'All Home Conferences')
%h1#tritonmun
  %span#main Home Conferences
.narrow
  = will_paginate
  - if signed_in?
    / Admin viewing others'
    - if current_user.admin?
      %span.admin
        = link_to "Add new home conference", "/tritonmun/new"
  %br/
  %br/
  /
    <#%= collection_select_tag(:user, :role, User.all,
    \:id, :role, {}, {:name=>'user_filter'}) %>
  %ul.users= render @hconferences
  = will_paginate
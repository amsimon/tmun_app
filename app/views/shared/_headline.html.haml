%li
  %h2
    = headline.title
  %a{:href => "#{headline.button_url}"}
    .apply
      = headline.button_text
    %br/
    = link_to "edit", edit_branch_headline_path(parent, headline), class: "admin-link"
    = link_to "delete", headline, method: :delete, data: { confirm: "You sure?" }, class: "admin-link"

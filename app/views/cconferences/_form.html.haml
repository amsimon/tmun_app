= form_for(@cconference) do |f|
  = render 'shared/error_messages', object: f.object
  %table.sign
    %tr
      %td= f.label :number
      %td= f.text_field :number
    %tr
      %td= f.label :season
      %td= f.text_field :season
    %tr
      %td= f.label :year
      %td= f.text_field :year
    %tr
      %td= f.label :date
      %td= f.text_field :date
    %tr
      %td= f.label :location
      %td= f.text_field :location
    %tr
      %td= f.label :delegation_fee, "Delegation Fee"
      %td
        $#{f.text_field :delegation_fee}
    %tr
      %td= f.label :early_price, "Early Price"
      %td
        $#{f.text_field :early_price}
    %tr
      %td= f.label :early_date, "Early Price Deadline"
      %td
        Before: #{f.text_field :early_date}
    %tr
      %td= f.label :normal_price, "Normal Price"
      %td
        $#{f.text_field :normal_price}
    %tr
      %td= f.label :normal_date, "Normal Price Deadline"
      %td
        Before: #{f.text_field :normal_date}
    %tr
      %td= f.label :late_price, "Late Price"
      %td
        $#{f.text_field :late_price}
    %tr
      %td= f.label :late_date, "Late Price Deadline"
      %td
        Before: #{f.text_field :late_date}
    %tr
      %td= f.label :reg_url, "Reg Embed URL"
      %td= f.text_field :reg_url
  %br/
  %p= f.submit "Submit"